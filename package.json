{
  "name": "pd-react-skeleton",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@pagerduty/design-system": "^1.2.0",
    "@pagerduty/eslint-plugin-pagerduty": "3.1.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "jest-junit": "5.2.0",
    "pretender": "2.1.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "5.1.0",
    "react-scripts": "2.1.1",
    "react-test-renderer": "16.6.1",
    "reactstrap": "^6.5.0",
    "redux": "4.0.1",
    "redux-mock-store": "1.5.3",
    "redux-thunk": "2.3.0"
  },
  "scripts": {
    "start": "REACT_APP_ENV=development react-scripts start",
    "build": "react-scripts build",
    "build:ci:staging": "PUBLIC_URL=\"https://d30g1epxm0pa0g.cloudfront.net/react-skeleton/$CIRCLE_SHA1\" react-scripts build",
    "build:ci:production": "PUBLIC_URL=\"https://d30g1epxm0pa0g.cloudfront.net/react-skeleton\" react-scripts build",
    "deploy:staging": "npm run build:ci:staging && aws s3 cp build/ s3://pd-frontendcore-stg/react-skeleton/$CIRCLE_SHA1 --recursive --acl public-read --cache-control 'max-age=0, no-cache'",
    "deploy:production": "npm run build:ci:production && aws s3 cp build/ s3://pd-frontendcore-stg/react-skeleton --recursive --acl public-read --cache-control 'max-age=0, no-cache'",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --ci --reporters=default --reporters=jest-junit",
    "eject": "react-scripts eject",
    "lint": "eslint src/",
    "lint:ci": "eslint src/ --format junit -o test-results/eslint/eslint-results.xml",
    "lint-fix": "eslint src/ --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0"
  }
}
